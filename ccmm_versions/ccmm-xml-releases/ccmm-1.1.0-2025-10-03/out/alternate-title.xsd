<?xml version='1.0' encoding='utf-8' standalone='no'?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema">
  <xs:complexType name="alternate_title">
    <xs:sequence>
      <xs:element minOccurs="0" name="alternate_title_type" type="alternate_title_type"/>
      <xs:element minOccurs="0" name="iri" type="anyURI"/>
      <xs:element maxOccurs="unbounded" name="title">
        <xs:complexType>
          <xs:simpleContent>
            <xs:extension base="xs:string">
              <xs:attribute ref="xml:lang" use="required"/>
            </xs:extension>
          </xs:simpleContent>
        </xs:complexType>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:element name="alternate_title" type="alternate_title"/>
</xs:schema>
