<?xml version='1.0' encoding='utf-8' standalone='no'?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema">
  <xs:complexType name="funding_reference">
    <xs:sequence>
      <xs:element minOccurs="0" name="award_title" type="string"/>
      <xs:element maxOccurs="unbounded" name="funder" type="agent"/>
      <xs:element minOccurs="0" name="funding_program" type="string"/>
      <xs:element minOccurs="0" name="iri" type="anyURI"/>
      <xs:element minOccurs="0" name="local_identifier" type="string"/>
    </xs:sequence>
  </xs:complexType>
  <xs:element name="funding_reference" type="funding_reference"/>
</xs:schema>
