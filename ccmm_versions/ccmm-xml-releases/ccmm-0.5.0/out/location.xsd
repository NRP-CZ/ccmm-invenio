<?xml version='1.0' encoding='utf-8' standalone='no'?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema">
  <xs:complexType name="location">
    <xs:sequence>
      <xs:element maxOccurs="unbounded" minOccurs="0" name="bbox" type="anySimpleType"/>
      <xs:element minOccurs="0" name="geometry" type="geometry"/>
      <xs:element minOccurs="0" name="iri" type="anyURI"/>
      <xs:element maxOccurs="unbounded" minOccurs="0" name="location_name" type="string"/>
      <xs:element name="location_relation_type" type="location_relation_type"/>
      <xs:element maxOccurs="unbounded" minOccurs="0" name="related_object" type="resource"/>
    </xs:sequence>
  </xs:complexType>
  <xs:element name="location" type="location"/>
</xs:schema>
