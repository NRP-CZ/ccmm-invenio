<?xml version='1.0' encoding='utf-8' standalone='no'?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema">
  <xs:complexType name="geometry">
    <xs:sequence>
      <xs:element minOccurs="0" ref="gml:AbstractGeometry"/>
      <xs:element minOccurs="0" name="iri" type="anyURI"/>
      <xs:element maxOccurs="unbounded" minOccurs="0" name="label">
        <xs:complexType>
          <xs:simpleContent>
            <xs:extension base="xs:string">
              <xs:attribute ref="xml:lang" use="required"/>
            </xs:extension>
          </xs:simpleContent>
        </xs:complexType>
      </xs:element>
      <xs:element minOccurs="0" name="wkt">
        <xs:complexType>
          <xs:simpleContent>
            <xs:extension base="xs:string">
              <xs:attribute name="srsName" type="anyURI" use="optional"/>
            </xs:extension>
          </xs:simpleContent>
        </xs:complexType>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:element name="geometry" type="geometry"/>
</xs:schema>
